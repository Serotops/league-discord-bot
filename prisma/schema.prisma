// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id              String @id @default(cuid())
  discordId       String @unique
  youtubeId       String @unique
  youtubeName     String
  subscriberCount Int
  league          String
  accessToken     String
  refreshToken    String
}
